{
  "env": {
    "mocha": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "func-names": 0,
    "max-len": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "no-var": 2,
    "object-curly-spacing": 0,
    "no-else-return": 0,
    "consistent-return": 0
  },
  "parser": "babel-eslint",
  "extends": "airbnb/base"
}
